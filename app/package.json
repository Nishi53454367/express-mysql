{
  "name": "express-typeorm-jest",
  "version": "1.0.0",
  "description": "express-typeorm-jest sample",
  "main": "app/server.ts",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.35",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typeorm": "^0.2.31",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "start": "ts-node-dev server.ts",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "migrate:generate": "ts-node node_modules/.bin/typeorm migration:generate",
    "migrate:run": "ts-node node_modules/.bin/typeorm migration:run",
    "migrate:rollback": "ts-node node_modules/.bin/typeorm migration:revert",
    "seed:config": "ts-node node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "devDependencies": {
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.21",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4"
  }
}